{
    "manifest_version": 3,
    "name": "Webpage Subject Classifier",
    "version": "1.0",
    "description": "Classify web pages into Mathematics, Language Arts, Social Studies, or Science using an AI model.",
    "permissions": ["scripting", "activeTab", "storage", "aiLanguageModelOriginTrial"],
    "trial_tokens" : ["<trial-token>"],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
