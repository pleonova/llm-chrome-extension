{
    "manifest_version": 3,
    "name": "Webpage Subject Classifier",
    "version": "1.0",
    "description": "Classify web pages into Mathematics, Language Arts, Social Studies, or Science using an AI model.",
    "permissions": [
        "scripting", 
        "activeTab", 
        "storage",
        "identity"
    ],
    "host_permissions": [
        "<all_urls>",
        "https://sheets.googleapis.com/*"
    ],
    "oauth2": {
        "client_id": "753208981190-htqulgevh5k0s7l44dl2d63dkefqfev7.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/spreadsheets"
        ]
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
